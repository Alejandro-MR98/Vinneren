<div id="<%= dom_id product %>">
  <p>
    <strong>Product name:</strong>
    <%= product.product_name %>
  </p>

  <p>
    <strong>Material number:</strong>
    <%= product.material_number %>
  </p>

  <p>
    <strong>Stock:</strong>
    <%= product.stock %>
  </p>

  <p>
    <strong>Sup sub category:</strong>
    <%= product.sup_sub_category_id %>
  </p>

</div>
